{
  "name": "@pyviz/jupyterlab_pyviz",
  "version": "1.0.0-alpha.6",
  "description": "A JupyterLab extension for rendering PyViz content",
  "author": "Philipp Rudiger <prudiger@anaconda.org>",
  "main": "lib/index.js",
  "license": "BSD-3-Clause",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "bokeh",
    "bokehjs",
    "holoviews"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyviz/jupyterlab_pyviz.git"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.6",
    "@jupyterlab/apputils": "^1.0.0-alpha.6",
    "@jupyterlab/docregistry": "^1.0.0-alpha.6",
    "@jupyterlab/notebook": "^1.0.0-alpha.6",
    "@jupyterlab/rendermime-interfaces": "^1.3.0-alpha.6",
    "@jupyterlab/services": "^4.0.0-alpha.6",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/widgets": "^1.6.0",
    "@types/jquery": "^2.0.48",
    "@types/nouislider": "^9.0.4",
    "jquery": "^3.3.1",
    "nouislider": "^9.0.4"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "^3.4.3"
  }
}
