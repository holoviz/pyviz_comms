{
  "name": "@pyviz/jupyterlab_pyviz",
  "version": "1.0.5",
  "description": "A JupyterLab extension for rendering PyViz content",
  "author": "Philipp Rudiger <prudiger@anaconda.org>",
  "license": "BSD-3-Clause",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "bokeh",
    "bokehjs",
    "holoviews"
  ],
  "scripts": {
    "build": "tsc -b --verbose",
    "webpack": "webpack -p",
    "clean": "rimraf lib",
    "docs": "typedoc --options tdoptions.json --theme ../../typedoc-theme src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "main": "lib/index.js",
  "files": [
    "{dist,lib}/**/*.{js,js.map,d.ts}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyviz/jupyterlab_pyviz.git"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/docregistry": "^2.0.0",
    "@jupyterlab/notebook": "^2.0.0",
    "@jupyterlab/rendermime-interfaces": "^2.0.0",
    "@jupyterlab/services": "^5.0.0",
    "@lumino/coreutils": "^1.3.1",
    "@lumino/disposable": "^1.3.1",
    "@lumino/widgets": "^1.9.3",
    "@types/jquery": "^3.3.34",
    "@types/node": "^13.13.5",
    "@types/nouislider": "^9.0.4",
    "nouislider": "^9.0.4"
  },
  "peerDependencies": {
    "@jupyter-widgets/jupyterlab-manager": "^2.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "~3.8.3",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@jupyter-widgets/jupyterlab-manager": "^2.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
